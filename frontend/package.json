{
  "name": "@zutatensuppe/jigsaw-frontend",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "build:copyassets": "cpx \"src/assets/textures/*.{png,jpg,jpeg,svg,webp,mp3}\" ../build/public/assets/textures && cpx \"src/assets/featured-artist/*.{png,jpg,jpeg,svg,webp,mp3}\" ../build/public/assets/featured-artist && cpx \"src/assets/favicon/*.{png,jpg,jpeg,svg,webp,mp3}\" ../build/public/assets/favicon",
    "build": "vite build --config vite.config.build.mts && npm run build:copyassets",
    "start": "vite",
    "lint": "tsc --noEmit && eslint src && stylelint \"src/**/*.scss\"",
    "test": ""
  },
  "author": "Zutatensuppe",
  "license": "ISC",
  "dependencies": {
    "@ckpack/vue-color": "^1.6.0",
    "@mdi/font": "^7.3.67",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "vue": "^3.5.10",
    "vue-router": "^4.4.5",
    "vue3-toastify": "^0.2.3",
    "vuetify": "^3.7.2"
  },
  "devDependencies": {
    "@babel/types": "^7.25.6",
    "@eslint/js": "^9.11.1",
    "@types/lodash": "^4.17.9",
    "@vitejs/plugin-vue": "^5.1.4",
    "cpx2": "^7.0.1",
    "eslint": "^9.11.1",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "sass": "^1.79.4",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "~5.5.0",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.1",
    "vue-tsc": "^2.1.6"
  }
}
