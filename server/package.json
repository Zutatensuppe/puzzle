{
  "name": "@zutatensuppe/jigsaw-server",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "build:copyassets": "cpx \"node_modules/@img/**/*\" ../build/server/node_modules/@img",
    "build:app": "esbuild --sourcemap src/main.ts --external:pg-native --platform=node --bundle --minify --outfile=../build/server/main.js",
    "build": "npm run build:app && npm run build:copyassets",
    "start": "ts-node-dev --respawn src/main.ts",
    "video": "ts-node-dev src/video.ts",
    "lint": "tsc --noEmit && eslint src",
    "test": "vitest --run"
  },
  "author": "Zutatensuppe",
  "license": "ISC",
  "dependencies": {
    "@twurple/api": "^7.1.0",
    "@twurple/auth": "^7.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "exif": "^0.6.0",
    "express": "^4.18.2",
    "find-config": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "path2d-polyfill": "^2.0.1",
    "pg": "^8.11.0",
    "probe-image-size": "^7.2.3",
    "request": "^2.88.2",
    "sharp": "^0.33.1",
    "sib-api-v3-sdk": "^8.5.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/exif": "^0.6.3",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/pg": "^8.10.1",
    "@types/probe-image-size": "^7.2.0",
    "@types/request": "^2.48.8",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "cpx2": "^4.2.3",
    "esbuild": "^0.17.19",
    "eslint": "^8.41.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  }
